{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": ["www/**/*"],
  "exclude": ["**/www/manual_lib/*", "**/node_modules/*", "**/dist/*"],
}
